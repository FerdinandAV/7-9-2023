//3.a 
int a = 5;
int b = 5;
//3.b
int max = 10;
int min = 5;
//3.c
int x = 15;
float y = 7.5;
float z = 7.5;

//3.a
if (a==10 || b==10 || a+b==10) { println("Success"); }
else { println("Faliure"); }
//3.b
if (min<=5 || max<=5 && min+max>10) { println("Success"); }
else { println("Faliure"); }
//3.c
if (x==10 && x==20 && x==30 || y==10 && y==20 && y==30 || z==10 && z==20 && z==30) { println("Faliure"); }
else if (x+y+z==30) { println("Success"); }