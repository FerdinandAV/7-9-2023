void setup() {
  methodOne();
  methodTwo(); 
}

//The following method has an error in it. Fix the error and run it. 

void methodOne()
{
  int i = 1000; // You are not allowed to change this line. 
  
  int max = 10;
  
  if (i > max) {
    String output = "i is greater than " + max + ".";   
println(output);  
}
 
}
/*
  Finish the following method so that we can change the number assigned 
  to the weekday and it prints the correct output.  
*/
int weekDay = 0;

void methodTwo() 
{
  
  switch(weekDay) {
case 0:
  println("Today is monday");
  break;
case 1:
  println("Today is tuesday");
  break;
case 2:
  println("Today is wednesday");
  break;
case 3:
  println("Today is tuesday");
  break;
case 4:
  println("Today is friday");
  break;
case 5:
  println("Today is saturday");
  break;
case 6:
  println("Today is sunday");
 default:
 println("Invalid information");
  }
  boolean weekend = false;
  
  if (weekDay < 4) {
    weekend = false;
  }
  else {
    weekend = true;
  }
  
  // Print the name of the weekday here: 
  if (weekDay==0){
  println("Monday");
  }
  else if(weekDay==1){
  println("Tuesday");
  }
  else if(weekDay==2){
  println("Wednesday");
  }
  else if(weekDay==3){
  println("Tuesday");
  }
  else if(weekDay==4){
  println("Friday");
  }
  else if(weekDay==5){
  println("Saturday");
  }
  else if(weekDay==6){
  println("Sunday");
  }
  // Print if it is weekend here: 
  if (weekDay<4) {
  println("It is the weekend!");
  }
}